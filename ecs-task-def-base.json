{
  "family": "backend-task-def",
  "containerDefinitions": [
    {
      "name": "qrust-backend",
      "image": "ghcr.io/shashankmishra/backend/backend:ci",
      "repositoryCredentials": {
        "credentialsParameter": "arn:aws:secretsmanager:ap-south-1:947931883961:secret:ecs_ghcr_pull_token-jjBw8O"
      },
      "secrets": [
        {
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:947931883961:secret:pg_client_id-EL68Z6:CLIENT_ID::",
          "name": "CLIENT_ID"
        },
        {
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:947931883961:secret:pg_client_secret-evRdNo:CLIENT_SECRET::",
          "name": "CLIENT_SECRET"
        },
        {
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:947931883961:secret:phonepe-pg-prod-HcfPsc:PHONEPE_CLIENT_ID::",
          "name": "PHONEPE_CLIENT_ID"
        },
        {
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:947931883961:secret:phonepe-pg-prod-HcfPsc:PHONEPE_CLIENT_SECRET::",
          "name": "PHONEPE_CLIENT_SECRET"
        },
        {
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:947931883961:secret:phonepe-pg-prod-HcfPsc:PHONEPE_WEBHOOK_USERNAME::",
          "name": "PHONEPE_WEBHOOK_USERNAME"
        },
        {
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:947931883961:secret:phonepe-pg-prod-HcfPsc:PHONEPE_WEBHOOK_PASSWORD::",
          "name": "PHONEPE_WEBHOOK_PASSWORD"
        },
        {
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:947931883961:secret:message-central-prod-OD5vQX:MESSAGE_CENTRAL_AUTH_TOKEN::",
          "name": "MESSAGE_CENTRAL_AUTH_TOKEN"
        },
        {
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:947931883961:secret:meta-secret-prod-TCQ7Ca:WA_SYSTEM_USER_TOKEN::",
          "name": "WA_SYSTEM_USER_TOKEN"
        }
      ],
      "cpu": 512,
      "memory": 1024,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/backend",
          "mode": "non-blocking",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::947931883961:role/ecsTaskRole",
  "executionRoleArn": "arn:aws:iam::947931883961:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}